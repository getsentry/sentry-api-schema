{
  "Organization": {
    "type": "object",
    "properties": {
      "avatar": {
        "$ref": "avatar.json#/Avatar"
      },
      "dateCreated": {
        "type": "string",
        "format": "date-time",
        "example": "2018-11-06T21:20:08.115Z"
      },
      "id": {
        "type": "string",
        "example": "3"
      },
      "isEarlyAdopter": {
        "type": "boolean",
        "example": false
      },
      "name": {
        "type": "string",
        "example": "The Interstellar Jurisdiction"
      },
      "require2FA": {
        "type": "boolean",
        "example": false
      },
      "slug": {
        "type": "string",
        "example": "the-interstellar-jurisdiction"
      },
      "status": {
        "$ref": "organization-status.json#/OrganizationStatus"
      }
    }
  }
}
